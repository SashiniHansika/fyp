<?xml version="1.0" encoding="utf-8"?>
<er name="ER_name">
	<entity name="department">
		<attribute name="name"></attribute>
		<attribute name="number"></attribute>
		<attribute name="department"></attribute>
		<attribute name="locations" value="primary_key"></attribute>
		<attribute name="location" value="multi-valued"></attribute>
	</entity>
	<entity name="employee">
		<attribute name="name" type="composite_parent">
			<attribute name="first" type="composite_child"></attribute>
			<attribute name="middle" type="composite_child"></attribute>
			<attribute name="last" type="composite_child"></attribute>
		</attribute>
		<attribute name="number" value="primary_key"></attribute>
		<attribute name="address"></attribute>
		<attribute name="salary"></attribute>
		<attribute name="gender"></attribute>
		<attribute name="birth"></attribute>
	</entity>
	<entity name="project">
		<attribute name="name"></attribute>
		<attribute name="number" value="primary_key"></attribute>
		<attribute name="location"></attribute>
	</entity>
	<entity name="dependent">
		<attribute name="we"></attribute>
		<attribute name="name"></attribute>
		<attribute name="gender"></attribute>
		<attribute name="relationship"></attribute>
		<attribute name="number"></attribute>
	</entity>
</er>